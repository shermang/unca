/** page structure and layout
-------------------------------------------------------------------------------------- **/
#header {
background-color:#fff; 
background:url(../imgs/banners/banner.php) no-repeat; 
margin-bottom:-5px;
}

#footer {
text-align:center; 
color:#fff; 
width:100%; 
background-color:#0f2b65; 
padding:10px 0;
clear:both;
}

#footer a {
color:#fff;
}

#content {
margin-bottom:5px;
}

#main {
padding:40px 20px 20px 20px; 
width:680px; 
margin-bottom:2px; 
}

.top-stripes {
background:#fff url(../imgs/page-stripes-topright.png) top right no-repeat;
}

.bottom-stripes {
background:url(../imgs/page-stripes-bottomleft.png) no-repeat bottom left;
}

/** primary, secondary and utility navigation styles
-------------------------------------------------------------------------------------- **/
#logo {
margin:27px 0 29px 20px;
width:285px;
float:left;
}

#navigation-primary li,
#navigation-utility li,
.navigation-secondary li {
margin-left:0px;
}

#footer ul {
text-align:center; 
margin:0 auto 10px auto; 
list-style-type:none;
display:block;
}

#footer li {
border-right:solid #4572c5 1px;
margin-left:0px;
display:inline;
}

#footer li.last {
border-right:0px;
}

#footer li a {
padding:10px 15px;
}

#navigation-utility-wrapper {
float:right;
width:260px;
text-align:right;
padding:90px 10px 0px 0px; 
color:#fff;
}

#navigation-utility {}

#navigation-utility li {
border-right:solid #fff 1px;
}

#navigation-utility li.last {
border-right:0px;
}

#navigation-utility li a {
padding:10px 10px; 
color:#fff;
}

#navigation-primary-wrapper {
border:solid #fff 1px; 
margin-bottom:5px; 
padding:0; 
height:30px;
background:url(../imgs/navigation-primary-background-sprites.png) no-repeat 0 0; 
}

#navigation-primary li a {
background:url(../imgs/navigation-primary-background-sprites.png) no-repeat; 
padding-top:30px; 
height:0px; 
overflow:hidden; 
display:block;
}

#navigation-primary #about {
background-position:0px 0px; 
width:71px;
}

#navigation-primary #about:hover,
#navigation-primary #about.current {
background-position:0px -32px;
}

#navigation-primary #academics {
background-position:-71px 0px; 
width:92px;
}

#navigation-primary #academics:hover,
#navigation-primary #academics.current {
background-position:-71px -64px;
}

#navigation-primary #admissions {
background-position:-163px 0px; 
width:102px;
}
#navigation-primary #admissions:hover,
#navigation-primary #admissions.current {
background-position:-163px -96px;
}

#navigation-primary #life-on-campus {
background-position:-265px 0px; 
width:126px;
}

#navigation-primary #life-on-campus:hover,
#navigation-primary #life-on-campus.current {
background-position:-265px -128px;
}

#navigation-primary #athletics {
background-position:-391px 0px; 
width:90px;
}

#navigation-primary #athletics:hover,
#navigation-primary #athletics.current {
background-position:-391px -160px;
}

#navigation-primary #news-and-events {
background-position:-481px 0px; 
width:123px;
}

#navigation-primary #news-and-events:hover,
#navigation-primary #news-and-events.current {
background-position:-481px -192px;
}

#navigation-primary #centers {
background-position:-604px 0px; 
width:82px;
}

#navigation-primary #centers:hover,
#navigation-primary #centers.current {
background-position:-604px -224px;
}

#navigation-primary #search {
background:url(../imgs/navigation-primary-background-sprites.png) no-repeat -686px 0px; 
width:265px; 
height:30px; 
padding-right:5px; 
overflow:hidden;
}

#navigation-primary #search ul {
margin-top:-20px;
}

#navigation-primary #search li {
margin-left:5px;
}

#navigation-primary #search #q, #navigation-primary #search #query_string {
border:0px; 
padding:3px; 
margin-top:4px; 
width:190px; 
}

#navigation-primary #search-button-people,
#navigation-primary #search-button-web {
margin-top:4px;
}


.navigation-secondary {
margin-bottom:0px;
background:url(../imgs/navigation-secondary-leftcap.png) repeat-y;
padding-left:2px;
}

.navigation-secondary ul.menu {
margin-bottom:0px;
list-style-type:none;
}

.navigation-secondary li {
padding:0px 10px; 
border-left:solid #fff 1px; 
border-bottom:solid #fff 1px;
position:relative;
background:url(../imgs/navigation-secondary-background-sprites.png) repeat-x 0 0; 
text-transform:uppercase;
background-color:#008fd1; 
}

.navigation-secondary a {
color:#fff; 
padding:5px 0px; 
display:block; 
width:190px;
}

.navigation-secondary a.active {  /*override system style */
color:#FFFFFF;
}

.navigation-secondary ul.menu li {  /*override system style */
margin:0;
list-style:none;
}

.navigation-secondary .trigger {
position:absolute;
display:block;
top:0;
right:5px; 
width:12px; 
height:12px; 
margin-top:8px;
padding:0px;
cursor:pointer;
background:url(../imgs/navigation-secondary-arrow-down.png) no-repeat 0 0; 
}

.navigation-secondary .trigger-closed {
background:url(../imgs/navigation-secondary-arrow-right.png) no-repeat 0 0; 
}

.navigation-secondary ul li.active-trail {
background:url(../imgs/navigation-secondary-background-sprites.png) #ee5b0a repeat-x 0 -92px; 
}

.navigation-secondary ul.menu li ul li {
margin:auto -10px auto -11px;
text-transform:none;
}

.navigation-secondary ul li.active-trail ul li {
background:url(../imgs/navigation-secondary-background-sprites.png)#e78419 repeat-x 0 -274px; 
}

.navigation-secondary ul li ul li {
background:url(../imgs/navigation-secondary-background-sprites.png) #46add2 repeat-x 0 -183px; 
}

.navigation-secondary .first {
border-top:solid #fff 1px;
}

#navigation-secondary-bottom {
background:url(../imgs/navigation-secondary-bottomcap.png) repeat-x transparent;
height:2px;
display:block;
margin:0 auto 5px auto;
overflow:hidden;
width:100%;
}
/** in-page styles 
-------------------------------------------------------------------------------------- **/

/* section heading */
.section-heading {
color:#fff; 
background:url(../imgs/heading-background-left.png) no-repeat top left; 
height:34px; 
min-height:24px; 
margin:0 -5px;
padding:0 0 0 10px;
}

.section-heading span {
background:url(../imgs/heading-background-right.png) no-repeat top right; 
width:100%;
display:block;
font-style:normal;
text-transform:uppercase; 
font-size:12px;  
float:right; 
height:34px; 
min-height:34px; 
padding:7px 0 0 0;
}

.heading-for-highlight {
margin-bottom:0px;
}

/* highlight box */
.highlight-box {
background-color:rgb(232,238,225); /* fallback for IE */
background-color:rgba(232,238,225,0.7);
padding:10px 0px;
}

.highlight-box ul, .highlight-box div.view-footer {
   padding-left:10px;
}

/* messages */
.msg-general {
margin-bottom: 5px; 
padding:2px 0 5px 0;
clear:both;
}

.msg-general h4 {
text-transform:uppercase; 
padding:0 15px; 
font-style:normal;
}

.msg-general .message {
color:#333; 
background-color:#fff; 
padding:15px; margin-bottom:0;
}

.msg-general * {
margin:5px 5px 5px 5px;
}

.msg-emergency {
background: url(../imgs/msg-emergency-background.png) repeat-x #e91920; 
color:#fff;
}

.msg-emergency h4 {
font-weight:bold; 
font-size:14px; 
}

.msg-notice {
background: url(../imgs/msg-notice-background.png) repeat-x #ef8518; 
color:#fff;
}

.msg-notice h4 {
font-weight:normal; 
font-size:12px; 
}

/* article styles */
.article-title {
margin-bottom:10px;
}

.article-date {
color:#ef851a;
margin-bottom:10px;
}

/* calendar graphics */
.calendar-graphic {
background:transparent url(../imgs/calendar.png) no-repeat scroll left top;
display:block;
float:left;
height:48px;
position:relative;
text-align:center;
width:43px;
margin:0 5px 0 0;
}

.calendar-graphic span {
display:block;
text-transform:uppercase;
font-size:10px;
left:0;
position:absolute;
width:100%;
}

.calendar-graphic .calendar-month {
color:#fff;
top:-2px;
}

.calendar-graphic:hover {
text-decoration:none;
}

.calendar-graphic .calendar-day {
font-family:Georgia, "Times New Roman", Times, serif;
font-size:18px;
top:17px;
left:-1px;
}

.calendar-text {
margin:0 0 0 0;
display:block;
width:155px;
float:left;
}

.calendar-time {
font-weight:bold;
color:#000;
}

/* divider */
.divider {
background:url(../imgs/divider-left.png) no-repeat; 
height:13px; 
min-height:13px; 
width:100%
}

.divider span {
background:url(../imgs/divider-right.png) no-repeat; 
height:13px; 
min-height:13px; 
width:2px; 
display:block; 
float:right; 
margin-right:-2px;
}

/* columnar layouts */
.two-column .first-column {
width:330px; 
float:left;
}

.two-column .last-column {
width:330px; 
float:right;
}

.three-column .first-column,
.three-column .last-column {
float:left;
width:190px;
}

.three-column .interior-column {
float:left;
width:190px;
padding:0 20px;
}


.four-column .first-column,
.four-column .interior-column,
.four-column .last-column {
float:left; 
padding:0 20px; 
width:129px; 
border-right:solid #aaa 1px;
}

.four-column .last-column {
border:0px;
}

/* blue button */
.button-blue-rounded {
background:url(../imgs/button-rounded-blue-left.png) left top no-repeat;
height:29px;
float:left;
padding:0px 0px 0px 12px;
}
	
.button-blue-rounded span {
background:url(../imgs/button-rounded-blue-right.png) top right no-repeat;
float:right;
height:29px;
display:block;
margin: 0;
color:#fff;
padding:5px 12px 0 0;
margin:0 -12px 0 0;
}

/* captioned image */
.captioned-image-inline {
margin:0 20px 10px 0;
width:240px;
}

.captioned-image-inline p {
margin-bottom:0px;
background:url(../imgs/background-grey-transparent.png) repeat transparent;
color:#fff;
padding:5px 10px;
}

.captioned-image-full {
margin:0 0 20px 0;
width:680px;
}

.captioned-image-full p {
margin-bottom:0px;
background:url(../imgs/background-grey-transparent.png) repeat transparent;
color:#fff;
padding:5px 10px;
}

/* right nav spiff */
.right-spiff {
background-color:rgba(232,238,225,0.7);
padding:10px 15px;
}

.right-spiff h3 {
margin-bottom:5px;
color:#EF851A;
}

.right-spiff p {
margin-bottom:0px;
}
